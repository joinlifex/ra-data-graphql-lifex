{
    "name": "@joinlifex/ra-data-graphql-lifex",
    "version": "3.0.5",
    "description": "A GraphQL simple data provider for react-admin",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "default": "./dist/index.mjs"
        }
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/joinlifex/ra-data-graphql-lifex.git"
    },
    "bugs": {
        "url": "https://github.com/joinlifex/ra-data-graphql-lifex/issues"
    },
    "homepage": "https://github.com/joinlifex/ra-data-graphql-lifex#readme",
    "authors": [
        "Gildas Garcia",
        "LifeX"
